<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>

 <TableView fx:controller="de.chrb.gustav.controller.StatTableController" fx:id="statTable"
 		layoutX="34.0" layoutY="66.0" prefHeight="371.0" prefWidth="813.0"
 		xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">

   <columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/></columnResizePolicy>
    <columns>
    	<TableColumn fx:id="fileName" prefWidth="130.0" text="File" />
        <TableColumn fx:id="type" prefWidth="100.0" text="Name" />
        <TableColumn fx:id="num" prefWidth="75.0" text="Num" />
        <TableColumn fx:id="numPerc" prefWidth="100.0" text="Num (%)" />
        <TableColumn fx:id="tocalGc" prefWidth="88.0" text="Total GC (ms)" />
        <TableColumn fx:id="tocalGcPerc" prefWidth="88.0" text="Total GC (%)" />
        <TableColumn fx:id="overhead" prefWidth="120.0" text="Overhead" />
        <TableColumn fx:id="avg" prefWidth="101.0" text="Avg (ms)" />
        <TableColumn fx:id="sigma" prefWidth="110.0" text="Sigma (ms)" />
        <TableColumn fx:id="min" prefWidth="103.0" text="Min (ms)" />
        <TableColumn fx:id="max" prefWidth="141.0" text="Max (ms)" />
    </columns>
  </TableView>